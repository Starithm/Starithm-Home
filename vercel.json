{
  "buildCommand": "npm run build:all",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/novatrace",
      "destination": "/novatrace/index.html"
    },
    {
      "source": "/novatrace/",
      "destination": "/novatrace/index.html"
    },
    {
      "source": "/novatrace/status",
      "destination": "/novatrace/index.html"
    },
    {
      "source": "/home",
      "destination": "/home/index.html"
    },
    {
      "source": "/home/",
      "destination": "/home/index.html"
    }
  ],
  "headers": [
    {
      "source": "/novatrace/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        }
      ]
    },
    {
      "source": "/home/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options", 
          "value": "SAMEORIGIN"
        }
      ]
    }
  ]
}
